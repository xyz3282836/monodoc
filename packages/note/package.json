  },
  "devDependencies": {
    "@vuepress/client": "2.0.0-beta.60",
    "vue": "^3.2.47",
    "vuepress": "2.0.0-beta.60",
    "vuepress-theme-hope": "2.0.0-beta.177"
  }
}